{
  "version": 1,
  "disable_existing_loggers": "False",
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "INFO",
      "formatter": "detailed",
      "stream": "ext://sys.stdout",
      "filters": ["add_context"]
    }
  },
  "formatters": {
    "detailed": {
      "format": "%(asctime)s - [%(threadName)s] [%(thread)d] - %(name)s - %(levelname)s in %(filename)s:%(lineno)s %(funcName)s(): submission: %(submission_id)s: %(message)s"
    }
  },
  "root" : {
    "level": "INFO",
    "handlers": ["console"] 
  },
  "loggers": {
    "ingest": {
      "level": "DEBUG"
    },
    "ingest.api.ingestapi": {
      "level": "INFO"
    },
    "broker.service.spreadsheet_upload_service": {
      "level": "INFO"
    },
    "broker.submissions.export_to_spreadsheet_service": {
      "level": "INFO"
    },
    "geo_to_hca": {
      "level": "INFO"
    },
    "broker.import_geo.routes": {
      "level": "INFO"
    }
  },
  "filters": {
    "add_context": {
      "()": "broker.common.util.logging.SessionContextFilter"
    }
  }
}